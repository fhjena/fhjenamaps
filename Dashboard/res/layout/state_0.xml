    />

<Button 
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
   	android:id="@+id/sf_Campus"
    android:text="@string/sf_Campus"
    android:onClick="wechsleActivity"
    android:enabled="false"
       
    />

</LinearLayout>